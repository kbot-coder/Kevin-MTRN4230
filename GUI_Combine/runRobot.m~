function runRobot(pickTarget,placeTarget)
if length(pickTarget(:,1))==length(placeData(:,1)),
    for i=1:length(pickData(:,1),
        pick(pickTarget(i,:))
        place(placeTarget(i,:))
    end
else
    errordlg('Unbalance Pick & Place Target'); 
end
return

function pick(pickTarget)
X = pickTarget(1);
Y = pickTarget(2);
theta = pickTarget(3);
run_vacuum_pump
go_to(X,Y,theta)
run_soneloid_valve


return